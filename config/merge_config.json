{
  "output_filename": "master_dataset.csv",
  "datasets": {
    "florida_storms_60nm_summary": {
      "filename": "florida_storms_60nm_summary.csv",
      "directory": "processed",
      "enabled": true,
      "columns": ["Year", "Storm", "ID", "Closest Distance (NM)", "Max Wind (kt)", "Min Pressure (mb)"]
    },
    "hurricane_economic_impacts_florida": {
      "filename": "hurricane_economic_impacts_1980_2024_florida_landfall.csv",
      "directory": "processed",
      "enabled": true,
      "columns": ["event_name", "begin_date", "end_date", "cost_usd_billion_cpi_adjusted", "deaths", "data_year"]
    },
    "florida_hurricanes_noaa": {
      "filename": "florida_hurricanes_1995_2025_noaa.csv",
      "directory": "processed",
      "enabled": true,
      "columns": ["storm_id", "name", "date_range", "max_category", "max_wind_speed", "min_pressure"]
    },
    "florida_storms_60nm_detail": {
      "filename": "florida_storms_60nm_2004_2025.csv",
      "directory": "processed",
      "enabled": false,
      "columns": ["storm_id", "storm_name", "date", "status", "lat", "lon", "max_wind", "min_pressure", "year", "distance_nm"]
    },
    "hurricane_economic_impacts_all": {
      "filename": "hurricane_economic_impacts_1980_2024.csv",
      "directory": "processed",
      "enabled": false,
      "columns": ["event_name", "begin_date", "end_date", "cost_usd_billion_cpi_adjusted", "deaths", "data_year"]
    },
    "florida_hurricane_proximity": {
      "filename": "florida_hurricane_proximity_summary.csv",
      "directory": "processed",
      "enabled": false,
      "columns": ["Year", "Storm", "Closest Distance (NM)", "Max Wind (kt)", "Min Pressure (mb)"]
    }
  },
  "merge_strategy": {
    "method": "concat",
    "notes": "Concatenates all enabled datasets vertically. Columns not present in a dataset will be filled with empty values. Set method to 'join' and configure join_key for horizontal merging."
  }
}
